<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Parallel test classes"
       parallel="classes"
       thread-count="2">
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>

    <test name="UI Tests">
        <classes>
            <class name="tests.LoginAndCartTest"/>
            <class name="tests.LoginTest"/>
        </classes>
    </test>

</suite>
